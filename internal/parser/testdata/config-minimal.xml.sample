<?xml version="1.0"?>
<opnsense>
  <version>24.1</version>
  <theme>opnsense</theme>
  <system>
    <hostname>Minimal</hostname>
    <domain>example.local</domain>
    <optimization>normal</optimization>
    <timezone>UTC</timezone>
    <timeservers>pool.ntp.org</timeservers>
    <webgui>
      <protocol>https</protocol>
    </webgui>
    <user>
      <name>admin</name>
      <descr>Administrator</descr>
      <scope>system</scope>
      <groupname>admins</groupname>
      <password>$2y$10$test.hash.here</password>
      <uid>0</uid>
    </user>
    <group>
      <name>admins</name>
      <description>Administrators</description>
      <scope>system</scope>
      <gid>0</gid>
      <member>0</member>
      <priv>page-all</priv>
    </group>
    <nextuid>2000</nextuid>
    <nextgid>2000</nextgid>
  </system>
  <interfaces>
    <wan>
      <enable>1</enable>
      <if>em0</if>
      <ipaddr>dhcp</ipaddr>
    </wan>
    <lan>
      <enable>1</enable>
      <if>em1</if>
      <ipaddr>192.168.1.1</ipaddr>
      <subnet>24</subnet>
    </lan>
  </interfaces>
  <dhcpd>
    <lan>
      <enable>1</enable>
      <range>
        <from>192.168.1.100</from>
        <to>192.168.1.199</to>
      </range>
    </lan>
  </dhcpd>
  <unbound>
    <enable>1</enable>
  </unbound>
  <nat>
    <outbound>
      <mode>automatic</mode>
    </outbound>
  </nat>
  <filter>
    <rule>
      <type>pass</type>
      <ipprotocol>inet</ipprotocol>
      <descr>Default allow LAN to any rule</descr>
      <interface>lan</interface>
      <source>
        <network>lan</network>
      </source>
      <destination>
        <any/>
      </destination>
    </rule>
  </filter>
  <rrd>
    <enable/>
  </rrd>
  <load_balancer>
    <monitor_type>
      <name>ICMP</name>
      <type>icmp</type>
      <descr>ICMP</descr>
      <options/>
    </monitor_type>
  </load_balancer>
  <ntpd>
    <prefer>pool.ntp.org</prefer>
  </ntpd>
  <widgets>
    <sequence>system_information-container:00000000-col3:show</sequence>
    <column_count>1</column_count>
  </widgets>
</opnsense>
